  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Elkia Auth Server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/elkia-authserver"
    },
    {
      "name": "Attach Elkia Auth Server",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "host": "localhost",
      "port": 56268,
      "cwd": "${workspaceFolder}/cmd/elkia-authserver"
    },
    {
      "name": "Launch Elkia Fleet",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/elkia-fleet"
    },
    {
      "name": "Attach Elkia Fleet",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "host": "localhost",
      "port": 56268,
      "cwd": "${workspaceFolder}/cmd/elkia-fleet"
    },
    {
      "name": "Launch Elkia Gateway",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/elkia-gateway"
    },
    {
      "name": "Attach Elkia Gateway",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "host": "localhost",
      "port": 56268,
      "cwd": "${workspaceFolder}/cmd/elkia-gatwaey"
    },
  ]
}
